{
  "alias": "php-coveralls.phar",
  "base-path": null,
  "chmod": "0755",
  "directories": [
    "bin",
    "src"
  ],
  "check-requirements": false,
  "compactors": [
    "KevinGH\\Box\\Compactor\\Php"
  ],
  "files": [
    "LICENSE"
  ],
  "finder": [
    {
      "name": ["*.php", "*.php8", "*.pem"],
      "exclude": ["tests"],
      "in": ["vendor"]
    }
  ],
  "git-version": "package_version",
  "main": "bin/php-coveralls",
  "output": "build/artifacts/php-coveralls.phar",
  "stub": true
}
